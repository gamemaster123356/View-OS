echo off
CHCP 65001
setlocal EnableDelayedExpansion
title View OS
set command=null
set osver=null
set kernelver=null
set build=null
if "%viewosopend%"== "1" goto :mainsys
set viewosopend=0
cd..
cd Data
ren os.dat os.bat
call os.bat
ren os.bat os.dat
cd..
cd Drivers
call VGA.bat
if not exist VGA.bat cd.. && cd Kernel && ren Kernel_boot.k Kernel_boot.bat && call Kernel_boot.bat && goto kernelpanic
cd..
cd System
rename Systemcm.bat System.cm
cd..
cd Boot
rename Boot_sector.bat Boot_sector.b
cls
